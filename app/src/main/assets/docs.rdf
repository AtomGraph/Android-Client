  <rdf:Description rdf:about="https://linkeddatahub.com:4443/docs/about">
    <j.4:inDataset rdf:resource="https://linkeddatahub.com:4443/docs/graphs/9d5fe121-b1e9-468d-a235-c0b67c6f7e46#this"/>
    <j.5:created rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-08-19T08:58:11.806+02:00</j.5:created>
    <j.0:owner rdf:resource="https://linkeddatahub.com:4443/admin/acl/agents/ce84eb31-cc1e-41f4-9e29-dacd417b9818#this"/>
